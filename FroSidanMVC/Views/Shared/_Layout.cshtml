<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
    <link rel="stylesheet" href="~/CSS/StyleSheet.css" />

</head>
<body>
    <nav class="navbar stucky-top navbar-expand-md justify-content-center">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" asp-controller="Products" asp-action="Index">Startsida</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="Products" asp-action="Shop">Bläddra</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="Members" asp-action="MyPages">Mina sidor</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="Products" asp-action="Checkout">Varukorg</a>
            </li>
        </ul>
        @*<partial name="_ShopCartBox" />*@
    </nav>

    <div>
        @RenderBody()
    </div>

    <footer class="footer footer-light bg-faded text-center">
        <p>
            <div class="container">
                <div class="col">
                    Fröbutiken 2021&trade;
                </div>
            </div>
        </p>
    </footer>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    @RenderSection("scripts", false)
</body>
</html>
